 <view class="page">
 <view class='bg-white1'>
    <block wx:for="{{recordList}}" wx:key="item.id" key="{{item.id}}">
      <navigator url="{{item.url}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
          <image src="{{item.icon}}" style="vertical-align: middle;width:30px; height: 30px;"></image>
        </view>
        <view class="weui-cell__bd pl15">
          <view class="font17 " style='color:#666666;'>{{item.name}}</view>
        </view>
        <view class="weui-cell__ft1 weui-cell__ft_in-access font14">{{item.desc}}</view>
      </navigator>
    </block>
  </view>
</view>